{
  "name": "kart",
  "version": "0.0.0",
  "scripts": {
    "test": "mocha --debug-brk --inspect --require esbuild-register --file testSetup.js src/**/*.test.js",
    "test:debug": "mocha --debug-brk --inspect --require esbuild-register --file testSetup.js src/**/*.test.js",
    "test:watch": "npm run test -- --watch --recursive ",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prettier": "prettier --write .",
    "lint": "eslint -f stylish -c .eslintrc.js --fix . --ext .js",
    "format": "npm run prettier && npm run lint",
    "pre-flight-check": "npm run format && npm run test",
    "prepare": "husky install"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "immer": "9.0.2",
    "kind-of": "6.0.3",
    "lodash": "4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-overlay": "^6.0.9",
    "stack-trace": "^1.0.0-pre1",
    "stackframe": "^1.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "chai": "^4.3.4",
    "esbuild": "^0.12.5",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^2.5.0",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "husky": "^6.0.0",
    "jest": "^27.0.1",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "vite": "^2.3.4"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run pre-flight-check",
    "*.{md,html,css,scss}": "npm run prettier"
  }
}
